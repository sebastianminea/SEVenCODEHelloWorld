<UserControl x:Class="SEVenCODEHelloWorld.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" 
    d:DesignHeight="600" d:DesignWidth="800">
    <UserControl.Resources>
        <BeginStoryboard x:Key="bohBeginStoryboard" >
            <Storyboard x:Name="mainStoryboard">
                <DoubleAnimation BeginTime="0:0:1" RepeatBehavior="Forever" AutoReverse="True" Duration="0:0:2" To="1" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="imgSEVenCODE" d:IsOptimized="True"/>
                <DoubleAnimation BeginTime="0:0:1" RepeatBehavior="Forever" AutoReverse="True" Duration="0:0:2" To="1" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="imgSEVenCODE" d:IsOptimized="True"/>
                <DoubleAnimation BeginTime="0:0:1" RepeatBehavior="Forever" AutoReverse="True" Duration="0:0:2" To="1" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleX)" Storyboard.TargetName="textBlock" d:IsOptimized="True"/>
                <DoubleAnimation BeginTime="0:0:1" RepeatBehavior="Forever" AutoReverse="True" Duration="0:0:2" To="1" Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.ScaleY)" Storyboard.TargetName="textBlock" d:IsOptimized="True"/>
                <ObjectAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="textBlock">
                    <DiscreteObjectKeyFrame KeyTime="0:0:2">
                        <DiscreteObjectKeyFrame.Value>
                            <Visibility>Visible</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                    </DiscreteObjectKeyFrame>
                    <DiscreteObjectKeyFrame KeyTime="0:0:2.9">
                        <DiscreteObjectKeyFrame.Value>
                            <Visibility>Collapsed</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                    </DiscreteObjectKeyFrame>
                    <DiscreteObjectKeyFrame KeyTime="0:0:3">
                        <DiscreteObjectKeyFrame.Value>
                            <Visibility>Visible</Visibility>
                        </DiscreteObjectKeyFrame.Value>
                    </DiscreteObjectKeyFrame>
                </ObjectAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.GlobalOffsetZ)" Storyboard.TargetName="txtZoom">
                    <EasingDoubleKeyFrame KeyTime="0" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:3" Value="7000"/>
                    <DiscreteDoubleKeyFrame KeyTime="0:0:4" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:5" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:6" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:7" Value="7000"/>
                    <DiscreteDoubleKeyFrame KeyTime="0:0:8" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:9" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:10" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:11" Value="7000"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.GlobalOffsetZ)" Storyboard.TargetName="txtZoom2">
                    <EasingDoubleKeyFrame KeyTime="0" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:3" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:4" Value="7000"/>
                    <DiscreteDoubleKeyFrame KeyTime="0:0:5" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:6" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:7" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:8" Value="7000"/>
                    <DiscreteDoubleKeyFrame KeyTime="0:0:9" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:10" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:11" Value="3000"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.GlobalOffsetZ)" Storyboard.TargetName="txtZoom3">
                    <EasingDoubleKeyFrame KeyTime="0" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:3" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:4" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:5" Value="7000"/>
                    <DiscreteDoubleKeyFrame KeyTime="0:0:6" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:7" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:8" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:9" Value="7000"/>
                    <DiscreteDoubleKeyFrame KeyTime="0:0:10" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:11" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames RepeatBehavior="Forever" Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.GlobalOffsetZ)" Storyboard.TargetName="txtZoom4">
                    <EasingDoubleKeyFrame KeyTime="0" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:2" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:3" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:4" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:5" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:6" Value="7000"/>
                    <DiscreteDoubleKeyFrame KeyTime="0:0:7" Value="-3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:8" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:9" Value="3000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:10" Value="7000"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:11" Value="10000"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
        </BeginStoryboard>
    </UserControl.Resources>
    <Grid x:Name="LayoutRoot" Background="Black">
        
        <TextBlock Name="txtZoom4" FontStretch="UltraExpanded" Foreground="DarkGreen" FontSize="50" TextWrapping="Wrap" RenderTransformOrigin="0.5,0.5">
            
            <TextBlock.Projection>
                <PlaneProjection  >
                </PlaneProjection>
            </TextBlock.Projection>
        </TextBlock>
        <TextBlock Name="txtZoom3" FontStretch="UltraExpanded" Foreground="DarkGreen" FontSize="50" TextWrapping="Wrap" RenderTransformOrigin="0.5,0.5">
            
            <TextBlock.Projection>
                <PlaneProjection  >
                </PlaneProjection>
            </TextBlock.Projection>
        </TextBlock>
        <Image Source="SEVenCODE.png" x:Name="imgSEVenCODE" RenderTransformOrigin="0.5,0.5" >
            <Image.RenderTransform>
                <CompositeTransform ScaleY="0" ScaleX="0"/>
            </Image.RenderTransform>
        </Image>
        <TextBlock x:Name="textBlock" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Center" FontSize="50" Text="Hello World">
            <TextBlock.RenderTransform>
                <CompositeTransform ScaleY="0" ScaleX="0"/>
            </TextBlock.RenderTransform>
        </TextBlock>
        <TextBlock Name="txtZoom2" FontStretch="UltraExpanded" Foreground="DarkGreen" FontSize="50" TextWrapping="Wrap" RenderTransformOrigin="0.5,0.5">
            
            <TextBlock.Projection>
                <PlaneProjection  >
                </PlaneProjection>
            </TextBlock.Projection>
        </TextBlock>
        <TextBlock Name="txtZoom" FontStretch="UltraExpanded" Foreground="DarkGreen" FontSize="50" TextWrapping="Wrap" RenderTransformOrigin="0.5,0.5">
            
            <TextBlock.Projection>
                <PlaneProjection >
                </PlaneProjection>
            </TextBlock.Projection></TextBlock>
       

    </Grid>
</UserControl>
